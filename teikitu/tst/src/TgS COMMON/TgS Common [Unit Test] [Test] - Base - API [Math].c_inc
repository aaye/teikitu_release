    Test__Expect_EQ( TYPE_K(0), FCM(CLP_MIN)( TYPE_K(0), -TYPE_K(4)));
    Test__Expect_EQ(-TYPE_K(3), FCM(CLP_MIN)(-TYPE_K(3), -TYPE_K(8)));
    Test__Expect_EQ(-TYPE_K(6), FCM(CLP_MIN)(-TYPE_K(6), -TYPE_K(8)));
