    VAR_T(C)        fA3434 = pmM1->_33 * pmM1->_44 - pmM1->_34 * pmM1->_43;
    VAR_T(C)        fA2434 = pmM1->_32 * pmM1->_44 - pmM1->_34 * pmM1->_42;
