/* ---- FCN_VO(tgGM_Intersection_Area_RT2D) -------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE VAR_T() FCN_VO(tgGM_Intersection_Area_RT2D)( VEC_OBJ_T(TgRECT2D,CPCU) psRT2D0, VEC_OBJ_T(TgRECT2D,CPCU) psRT2D1 )
{
    VAR_T()                             f00 = FCN_F(tgCM_MIN)(VEC_S_UN_1_MEMBER(psRT2D0->m_uRect).z, VEC_S_UN_1_MEMBER(psRT2D1->m_uRect).z);
    VAR_T()                             f01 = FCN_F(tgCM_MAX)(VEC_S_UN_1_MEMBER(psRT2D0->m_uRect).x, VEC_S_UN_1_MEMBER(psRT2D1->m_uRect).x);
    VAR_T()                             f02 = FCN_F(tgCM_MIN)(VEC_S_UN_1_MEMBER(psRT2D0->m_uRect).w, VEC_S_UN_1_MEMBER(psRT2D1->m_uRect).w);
    VAR_T()                             f03 = FCN_F(tgCM_MAX)(VEC_S_UN_1_MEMBER(psRT2D0->m_uRect).y, VEC_S_UN_1_MEMBER(psRT2D1->m_uRect).y);

    return FCN_F(tgCM_MAX)(0, f00 - f01) * FCN_F(tgCM_MAX)(0, f02 - f03);
}


