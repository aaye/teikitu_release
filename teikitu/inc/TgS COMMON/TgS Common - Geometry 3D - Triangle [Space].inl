/* ---- FCN_VO(tgGM_ST_Is_Contained) --------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgBOOL FCN_VO(tgGM_ST_Is_Contained)( VEC_OBJ_T(TgSTRI,CPCU) psST1, VEC_T(1,C) vS0 )
{
    if (FCN_V(tgMH_CMP_ALL_TO_BOOL)( FCN_V(tgMH_CMP_GE)( FCN_VO(tgGM_Sign_Dist_PN_PT)( psST1->m_avPlane + 0, vS0 ), FCN_V(tgMH_SET1)( TYPE_K(0) ) ) ))
    {
        return false;
    };

    if (FCN_V(tgMH_CMP_ALL_TO_BOOL)( FCN_V(tgMH_CMP_GE)( FCN_VO(tgGM_Sign_Dist_PN_PT)( psST1->m_avPlane + 1, vS0 ), FCN_V(tgMH_SET1)( TYPE_K(0) ) ) ))
    {
        return false;
    };

    if (FCN_V(tgMH_CMP_ALL_TO_BOOL)( FCN_V(tgMH_CMP_GE)( FCN_VO(tgGM_Sign_Dist_PN_PT)( psST1->m_avPlane + 2, vS0 ), FCN_V(tgMH_SET1)( TYPE_K(0) ) ) ))
    {
        return false;
    };

    return (true);
}


